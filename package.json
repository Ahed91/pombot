{
  "name": "pombot",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "get-secrets": "ssh nest.bocoup.com \"cat /mnt/secrets/{rds,pombot-slack,pombot-db-dev}\" > ./.env",
    "start": "node app.js",
    "migrate": "careen",
