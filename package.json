    "migrate:create": "careen -C"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "careen": "^0.1.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "pg": "^5.0.0",
    "pg-monitor": "^0.5.8",
    "pg-promise": "^4.5.0",
    "slack": "^7.2.0"
  }
}
